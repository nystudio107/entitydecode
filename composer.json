{
	"name": "nystudio107/entitydecode",
	"description": "A simple plugin for decoding HTML entities from within Craft CMS templates.",
	"type": "craft-plugin",
	"version": "3.0.0",
	"keywords": [
		"craft",
		"cms",
		"craftcms",
		"craft-plugin",
		"entities"
	],
	"support": {
		"docs": "https://github.com/nystudio107/entitydecode/blob/v1/README.md",
		"issues": "https://github.com/nystudio107/entitydecode/issues"
	},
	"license": "MIT",
	"authors": [
		{
			"name": "nystudio107",
			"homepage": "https://nystudio107.com/"
		}
	],
	"require": {
		"craftcms/cms": "^4.0.0-alpha.1",
		"php": "^8.0.2"
	},
	"autoload": {
		"psr-4": {
			"nystudio107\\entitydecode\\": "src/"
		}
	},
	"extra": {
		"name": "EntityDecode",
		"handle": "entitydecode",
		"schemaVersion": "1.0.0",
		"hasCpSettings": false,
		"hasCpSection": false,
		"changelogUrl": "https://raw.githubusercontent.com/nystudio107/entitydecode/v1/CHANGELOG.md",
		"class": "nystudio107\\entitydecode\\EntityDecode"
	},
	"config": {
		"allow-plugins": {
			"yiisoft/yii2-composer": true,
			"craftcms/plugin-installer": true
		}
	},
	"minimum-stability": "dev",
	"prefer-stable": true,
	"require-dev": {
		"craftcms/rector": "dev-main"
	}
}
